	string password;
};
struct empleador
{
	string usuario;
	string nombre;
	string apellido;
	int ci;
	int telefono;
};
struct oficio
{
	string codigo;
	string nombre;
	string descripcion;
};

struct empleado
{
	string usuario;
	string nombre;
	string apellido;
	int ci;
	int telefono;
	string direccion;
};
struct servicio
{
